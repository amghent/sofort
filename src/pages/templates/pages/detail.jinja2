{% extends "base.jinja2" %}

{% block content %}
    <div id="content-wrapper" class="container-fluid">
        <h1>{{ page.title }}</h1>

        {% if page.intro.strip() != "" %}
            <p class="lead">{{ page.intro }}</p>
        {% endif %}

        <p>{{ page.content }}</p>
    </div>
{% endblock %}